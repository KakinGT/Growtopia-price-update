										<div id="recaptcha" class="g-recaptcha" data-sitekey="6Lf0vqsUAAAAAF3MC9vyGBaHgeMOA1yMwykZvAA0" data-callback="reportPrice" data-size="invisible"><div class="grecaptcha-badge" data-style="bottomright" style="width: 256px; height: 60px; display: block; transition: right 0.3s ease 0s; position: fixed; bottom: 14px; right: -186px; box-shadow: gray 0px 0px 5px; border-radius: 2px; overflow: hidden;"><div class="grecaptcha-logo"><iframe src="https://www.google.com/recaptcha/api2/anchor?ar=1&amp;k=6Lf0vqsUAAAAAF3MC9vyGBaHgeMOA1yMwykZvAA0&amp;co=aHR0cHM6Ly9ncm93c3RvY2tzLnh5ejo0NDM.&amp;hl=en-GB&amp;v=-wV2EAWEOTlEtZh4vNQtn3H1&amp;size=invisible&amp;cb=bqe1rxnq6j9s" width="256" height="60" role="presentation" name="a-rywr1qyqk0z1" frameborder="0" scrolling="no" sandbox="allow-forms allow-popups allow-same-origin allow-scripts allow-top-navigation allow-modals allow-popups-to-escape-sandbox"></iframe></div><div class="grecaptcha-error"></div><textarea id="g-recaptcha-response" name="g-recaptcha-response" class="g-recaptcha-response" style="width: 250px; height: 40px; border: 1px solid rgb(193, 193, 193); margin: 10px 25px; padding: 0px; resize: none; display: none;"></textarea></div><iframe style="display: none;"></iframe></div>
			        					<button type="button" class="growButton growCancelButton closeReport">Cancel</button>&nbsp;&nbsp;
	 				<button type="submit" class="growButton reportButton">Report</button>
 				</form>
			</div>
		</div>
	</div>
				<script async="" src="https://www.google.com/recaptcha/api.js" defer="" type="text/javascript"></script>
			<script type="text/javascript">
		var version = "2.1.2.3.35(1)";
		function setCookie(name,value,days) {
		    var expires = "";
		    if (days) {
		        var date = new Date();
		        date.setTime(date.getTime() + (days*24*60*60*1000));
		        expires = "; expires=" + date.toUTCString();
		    }
		    document.cookie = name + "=" + (value || "")  + expires + "; path=/;SameSite=None;Secure=true";
		}
		function getCookie(name) {
		    var nameEQ = name + "=";
		    var ca = document.cookie.split(';');
		    for(var i=0;i < ca.length;i++) {
		        var c = ca[i];
		        while (c.charAt(0)==' ') c = c.substring(1,c.length);
		        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
		    }
		    return null;
		}
		function eraseCookie(name) {   
		    document.cookie = name+'=; Max-Age=-99999999;';  
		}
		function setItem(name){
			item = name;
		}

		var opened = 0;
		var mod = null;
		var radio = "wrong";
		var item = null;
		$(document).ready(function(){
			setTimeout(function(){
				$(".loader").fadeOut(function(){
					$(".loading").fadeOut();
				});
			}, 150);
									var cachedVersion = getCookie('modal');
			if(cachedVersion != version){
				$(".dark-bg").fadeIn(function(){
					$(".newsModal").animate({
				    	height: "toggle"
				  	}, 500);
				});
			}
			$(".newsBox .closeNews").click(function(){
				setCookie('modal', version, 365);
				$(".newsModal").animate({
					height: "toggle",
				}, 500, function(){
					$(".dark-bg").fadeOut();
				});
			});
						$(".reportBox .closeReport").click(function(){
				$(".reportBox .closeReport").attr("disabled", true);
				$(".reportModal").animate({
					height: "toggle",
				}, 500, function(){
					$(".dark-bg").fadeOut();
					$(".suggPrice").val("");
					$(".reportBox .closeReport").attr("disabled", false);
				});
			});
			$(".menuToggle").click(function() {
				if(mod != null){
					$(mod).parent().children(".options").animate({
					    height: ["toggle", "swing"]
					}, 200);
					mod = null;
				}
				opened = 1;
				$(this).parent().children(".options").animate({
				    height: ["toggle", "swing"]
				}, 200);
				mod = this;
        	});
			window.onclick = function(event) {
				opened = opened + 1;
				if (opened == 3) {
					$(mod).parent().children(".options").animate({
					    height: ["toggle", "swing"]
					}, 200);
					mod = null;
					opened = 0;
				}
				if(opened == 1){
					opened = 0;
				}
			}
			$(".reportPrice").click(function(){
				$(".dark-bg").fadeIn(function(){
					$(".reportModal").animate({
				    	height: "toggle"
				  	}, 500);
				});
			});

				        $('.reportItem').submit(function(e){
	            e.preventDefault();
				$(".reportButton").attr("disabled", "disabled");
				$(".reportButton").html("Reporting...");
	            	            grecaptcha.execute();
	            	        });
	    	
	        // Show loader when switching between pages
			$('a').click(function(e){
				var a = this;
				if($(a).hasClass("growButton")) return;
				if($(a).attr("target") == "_blank") return;
				e.preventDefault();
				$(".loading").fadeIn(function(){
					$(".loader").fadeIn();
					setTimeout(function(){
						window.location.assign($(a).attr("href"));
					}, 100);
				});
			});
			
			// Show loader when submitting search form
			$('#searchForItems').submit(function(e){
				$(".loading").fadeIn(function(){
					$(".loader").fadeIn();
				});
			});
		});
				function reportPrice(token){
			var reason = null;
			if(radio == "wrong"){
				var suggested = $(".suggPrice").val();
				var info = suggested.toLowerCase().replace("wl", "").replace("s", "").replace(/\s/g, "").split("/");
				if(info[0] == "" || isNaN(info[0])){
					$(".reportButton").html("Report");
					$(".reportButton").attr("disabled", false);
					return alert("Invalid quantity!");
				}
				if(info[1] == "" || isNaN(info[1])){
					$(".reportButton").html("Report");
					$(".reportButton").attr("disabled", false);
					return alert("Invalid price!");
				}
				reason = "Invalid price. Suggested Price: "+info[0]+"/"+info[1];
			}else if(radio == "needseed"){
				reason = "This item needs a seed";
			}else if(radio == "unexisting"){
				reason = "This item does not exist!";
			}else if(radio == "spritefix"){
				reason = "This item's sprite is faulty!";
			}else{
				reason = "Not specified";
			}
			var that = $(".reportButton");
	        $.ajax({
	            url: 'engine/report.php',
	            type: 'POST',
	            data: {
	                item:item,
	                reason:reason,
	                token:token
	            }
	        })
	        .done(function(data){
	            var result = $.trim(data);
	            var results = JSON.parse(result);
	            	            grecaptcha.reset();
	            	            if(results['success'] === true){
	                alert("Item has been successfully reported!");
					$(".reportModal").animate({
						height: "toggle",
					}, 500, function(){
						$(".dark-bg").fadeOut();
						$(that).html("Report");
						$(".suggPrice").val("");
						$(that).attr("disabled", false);
					});
	            }else if(results['success'] === false){
	                alert(results['reason']);
	                $(that).html("Report");
	                $(that).attr("disabled", false);
	            }
	        })
	        .fail(function(){
	            	            grecaptcha.reset();
	            	            $(this).attr("disabled", false);
	            alert('Please check your internet connection!');
	        });
		}
		$('.radio').on('change', function(e) {
			radio = $(this).attr("option");
			$(".radio").attr("disabled", true);
			if($(this).attr("option") == "unexisting"){
				$(".newPrice").animate({
				    	height: 0
				}, 500, function(){
				 	$(".newPrice").css("display", "none");
				 	$(".radio").attr("disabled", false);
				});
			}else if($(this).attr("option") == "needseed"){
				$(".newPrice").animate({
				    	height: 0
				}, 500, function(){
				 	$(".newPrice").css("display", "none");
				 	$(".radio").attr("disabled", false);
				});
			}else if($(this).attr("option") == "wrong"){
				$(".newPrice").css("display", "block");
				$(".newPrice").animate({
				  	height: 100
			 	}, 500, function(){
			 		$(".radio").attr("disabled", false);
			 	});
			}else if($(this).attr("option") == "spritefix"){
				$(".newPrice").animate({
				    	height: 0
				}, 500, function(){
				 	$(".newPrice").css("display", "none");
				 	$(".radio").attr("disabled", false);
				});
			}
		});
			</script>	
